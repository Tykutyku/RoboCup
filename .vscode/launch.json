{
	// Use IntelliSense to learn about possible attributes.
	// Hover to view descriptions of existing attributes.
	// For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
	"version": "0.2.0",
	"configurations": [
		{
			"type": "debugpy",
			"request": "launch",
			"program": "./app.py",
			"args": ["--browser-window"],
		}
		,
		{
			"name":"flask debug",
			"type":"debugpy",
			"request":"launch",
			"module": "flask",
			"env": {
				"FLASK_APP": "app.py",
				"FLASK_DEBUG": "1"
			},
			"args":["--debug","run"],
			"jinja":true,
			"autoStartBrowser":false
		}
	]
}